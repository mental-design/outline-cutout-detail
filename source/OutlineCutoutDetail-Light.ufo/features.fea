# 
# CALT
# 
lookup subops {  # Substitute operators
    # hyphen
    sub @uppercase hyphen' @uppercase by hyphen_cap;
    sub @uppercase  hyphen' @numerics by hyphen_cap;
    sub @numerics  hyphen' @uppercase by hyphen_cap;
    sub @numerics  hyphen' @numerics by hyphen_cap;

    # name@email
    sub @lowercase at' @lowercase by at_lc;

    # : - Time
    sub @numerics  colon' @numerics by colon_cap;

    # Move this to pictograms
    sub @comparitor hyphen' by hyphen_cap;
    sub hyphen' @comparitor by hyphen_cap;
} subops;

lookup fractions {
	sub @numerics' fraction by @sups;
	sub fraction @numerics' by @subs;
} fractions;

lookup commas {
    sub quote' s by apostrophe;
    sub @lowercase quote' d by apostrophe;
    sub @lowercase quote' l l by apostrophe;
    sub @lowercase quote' r e by apostrophe;
    sub @lowercase quote' v e by apostrophe;
    sub @lowercase n quote' t by apostrophe;
    sub I quote' m by apostrophe;
    sub I quote' d by apostrophe;
    sub I quote' l l by apostrophe;
    sub I quote' v e by apostrophe;
} commas;

feature calt {
    lookup subops;
    lookup fractions;
    lookup commas;
} calt;

