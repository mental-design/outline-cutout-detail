lookup DutchIJ {
    sub i j by ij;
    sub I J by IJ;
} DutchIJ;

lookup IJAcute {
    sub I_with_acute J' by J_with_acute;
    sub i_with_acute j' by j_with_acute;
    sub i_with_acute j_ss02' by j_ss02_with_acute;
} IJAcute;

feature locl {
    script latn;
    language NLD;

    lookup DutchIJ;
    lookup IJAcute;
} locl;

